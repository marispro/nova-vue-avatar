{
    "name": "marispro/nova-vue-avatar",
    "description": "Vue Avatar (https://eliep.github.io/vue-avatar/) Field wrapper for Laravel Nova (https://nova.laravel.com/)",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Marispro\\NovaVueAvatar\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Marispro\\NovaVueAvatar\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
